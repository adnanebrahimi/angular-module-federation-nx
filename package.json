{
  "name": "cac-pos",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "npm run start:shell",
    "start:one": "npm run script:angular && nx s plugin-one",
    "start:two": "npm run script:angular && nx s plugin-two",
    "start:three": "npm run script:angular && nx s three",
    "start:shell": "npm run script:angular && nx s shell -o",
    "start:all": "npm run script:angular && nx run-many --target=serve --all --parallel",
    "build": "npm run build:shell",
    "build:shell": "ng build --prod shell && npm run script:plugins",
    "build:all": "ng build --prod shell && ng build --prod plugin-one && ng build --prod plugin-two && ng build --prod three && npm run script:plugins",
    "serve:dist": "concurrently \"serve dist/apps/shell -l 4200 -s\" \"serve dist/apps/plugin-one -l 3000 -s\" \"serve dist/apps/plugin-two -l 3500 -s\" \"serve dist/apps/three -l 5500 -s\" ",
    "script:plugins": "ts-node scripts/plugins-packager.ts",
    "script:angular": "node scripts/angular-configurator.js",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
  "resolutions": {
    "webpack": "^5.4.0"
  },
  "dependencies": {
    "@angular-architects/module-federation": "^1.2.2",
    "@angular/animations": "^11.0.0",
    "@angular/common": "^11.0.0",
    "@angular/compiler": "^11.0.0",
    "@angular/core": "^11.0.0",
    "@angular/forms": "^11.0.0",
    "@angular/platform-browser": "^11.0.0",
    "@angular/platform-browser-dynamic": "^11.0.0",
    "@angular/router": "^11.0.0",
    "@nrwl/angular": "11.0.20",
    "concurrently": "^5.3.0",
    "rxjs": "~6.5.5",
    "serve": "^11.3.2",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.0",
    "@angular/cli": "~11.0.0",
    "@angular/compiler-cli": "~11.0.0",
    "@angular/language-service": "^11.0.0",
    "@nrwl/cli": "11.0.20",
    "@nrwl/cypress": "11.0.20",
    "@nrwl/jest": "11.0.20",
    "@nrwl/tao": "11.0.20",
    "@nrwl/workspace": "11.0.20",
    "@types/jest": "26.0.8",
    "@types/node": "12.12.38",
    "codelyzer": "^6.0.0",
    "cypress": "^5.5.0",
    "dotenv": "6.2.0",
    "eslint": "7.10.0",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.1",
    "ng-packagr": "^11.0.0",
    "prettier": "2.1.2",
    "ts-jest": "26.4.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
